    "build:components": "componentsjs-generator -i ../../.componentsignore.json -r cfile"
  },
  "bugs": {
    "url": "https://github.com/TREEcg/connectors/issues"
  },
  "dependencies": {
    "@treecg/connector-types": "^1.1.8",
    "watcher": "^2.2.2"
  }
