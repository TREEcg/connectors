  },
  "bugs": {
    "url": "https://github.com/TREEcg/connectors/issues"
  },
  "dependencies": {
    "@rdfjs/types": "^1.1.0",
    "@treecg/connector-file": "^1.0.7",
    "@treecg/connector-http": "^1.0.6",
    "@treecg/connector-kafka": "^1.2.6",
    "@treecg/connector-ldes": "^1.1.5",
    "@treecg/connector-types": "^1.1.8",
    "@treecg/connector-ws": "^1.1.8",
    "@treecg/types": "^0.4.2"
  },
  "devDependencies": {
    "@types/n3": "^1.10.4",
    "n3": "^1.17.0"
  }
}
